# Convex Configuration
# Convex 개발 환경 URL (npx convex dev 실행 시 자동 생성)
VITE_CONVEX_URL=https://your-deployment.convex.cloud

# Clerk Authentication
# Clerk 대시보드에서 발급받은 Publishable Key
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_key_here

# Google Maps API
# Google Cloud Console에서 발급받은 API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# PostHog Analytics (선택)
# PostHog 프로젝트에서 발급받은 API Key
VITE_POSTHOG_KEY=phc_your_posthog_key_here
VITE_POSTHOG_HOST=https://app.posthog.com

# AxiomFM Logging (선택)
# AxiomFM 대시보드에서 발급받은 토큰
VITE_AXIOM_TOKEN=your_axiom_token_here
VITE_AXIOM_DATASET=travel-planner-logs

# Resend Email (선택)
# Resend 대시보드에서 발급받은 API Key
VITE_RESEND_API_KEY=re_your_resend_key_here
